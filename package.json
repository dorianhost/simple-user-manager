  "private": false,
  "scripts": {
    "migrate:generate": "ts-node ./node_modules/.bin/typeorm migration:generate --config migrations/migrations-config -n",
    "migrate": "ts-node ./node_modules/.bin/typeorm migration:run --config migrations/migrations-config",
    "migrate:revert": "ts-node ./node_modules/.bin/typeorm migration:revert --config migrations/migrations-config",
    "test:integration": "NODE_ENV=test jest .*/test/integration/.*\\.test\\.ts --runInBand --config jest.config.json",
    "test:unit": "NODE_ENV=test jest .*/test/unit/.*\\.test\\.ts --config jest.config.json",
    "test:acceptance": "NODE_ENV=test jest .*/test/acceptance/.*\\.test\\.ts --runInBand --config jest.config.json",
    "test": "NODE_ENV=test jest --runInBand --config jest.config.json",
    "start:dev": "node -r ts-node/register -r dotenv-safe/config ./src/index.ts",
    "eslint": "eslint src/**/*.ts --fix"
  },
  "devDependencies": {
    "@types/faker": "^4.1.12",
    "@types/hapi__joi": "^17.1.0",
    "@types/jest": "^25.2.2",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.1",
    "@types/node": "^14.0.1",
    "@types/supertest": "^2.0.9",
    "@types/uuid": "^7.0.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "dotenv-safe": "^8.2.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-prettier": "^3.1.3",
    "faker": "^4.1.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "awilix": "^4.2.6",
